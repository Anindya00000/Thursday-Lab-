//WAP to print factorial of no from 1 to 10
package com;

import java.util.*;

public class FactorialFrom_1_to_10 
{
  public  static void main(String[] args)
  {
	 Scanner sc = new Scanner(System.in);
	 int f,n=10;
	 for(int i=1;i<=n;i++)
	 {
		 f=1;
		 for(int j=1;j<=i;j++)
		 {
			 f=f*j;
		 }
		 System.out.printf("Factorial of %2d is: %10d \n",i,f);
	 }
  }
}
